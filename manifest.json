{
  "manifest_version": 2,
  "name": "Red Panda Launcher",
  "version": "0.6.14",
  "applications": {
    "gecko": {
      "id": "{0750ffc2-fd55-47be-9c69-b30854938391}"
    }
  },
  "description": "",
  "icons": {
    "48": "redpanda-256.png",
    "96": "redpanda-256.png"
  },
  "permissions": [
    "<all_urls>",
    "bookmarks",
    "contextMenus",
    "cookies",
    "history",
    "sessions",
    "storage",
    "tabs",
    "topSites"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "redpanda.background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "redpanda.content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.png",
    "*.js"
  ]
}